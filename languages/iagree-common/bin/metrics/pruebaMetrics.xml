<met:metricXML xmlns:met="http:/www.isa.us.es/ada/metrics">
	<met:metric id="boolean" type="enumerated" >
		<met:value value="True"/>
		<met:value value="False"/>
	</met:metric>
	<met:metric id="percent" type="integer"  min="0" max="100" />
	<met:metric id="size" type="integer"  min="0" max="200" />
	<met:metric id="mediumInt" type="integer"  min="0" max="100" />
	<met:metric id="time" type="integer"  min="0" max="100" />
	<met:metric id="interfaces" type="enumerated" >
		<met:value value="SOAP"/>
		<met:value value="REST"/>
	</met:metric>
	<met:metric id="price" type="integer"  min="0" max="400" />
	<met:metric id="integer" type="integer"  min="0" max="500" />
	<met:metric id="countries" type="enumerated" >
		<met:value value="US"/>
		<met:value value="west_US"/>
		<met:value value="EU_Irland"/>
		<met:value value="Pacific_Asia"/>
		<met:value value="Southamerica"/>
		<met:value value="GovCloud_US"/>
	</met:metric>
	<met:metric id="supportType" type="enumerated" >
		<met:value value="Bronze"/>
		<met:value value="Silver"/>
		<met:value value="Gold"/>
		<met:value value="Platinum"/>
	</met:metric>
	<met:metric id="money" type="integer"  min="0" max="200" />
	<met:metric id="translations" type="enumerated" >
		<met:value value="ES_to_EN-UK"/>
		<met:value value="ES_to_EN-US"/>
		<met:value value="ES_to_FR"/>
		<met:value value="EN-UK_to_ES"/>
		<met:value value="EN-US_to_ES"/>
		<met:value value="FR_to_ES"/>
		<met:value value="Auto"/>
	</met:metric>
	<met:metric id="int" type="integer"  min="0" max="20" />
</met:metricXML>