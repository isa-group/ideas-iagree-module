<?xml version="1.0" encoding = "UTF-8"?>
<wsag:Template wsag:TemplateId="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
 xmlns:wsag="http://schemas.ggf.org/graap/2007/03/ws-agreement" 
 xmlns:xs="http://www.w3.org/2001/XMLSchema" 
 xsi:schemaLocation="http://schemas.ggf.org/graap/2007/03/wsagreement" 
 agreement_types.xsd="http://www.w3.org/2001/XMLSchema XMLSchema.xsd" >
	<wsag:Name >S3Premium</wsag:Name>
	<wsag:Context >
		<wsag:AgreementResponder >Provider</wsag:AgreementResponder >
		<wsag:GMTZone >+1</wsag:GMTZone >

	</wsag:Context >

	<wsag:Terms wsag:Name="AWS-S3PremiumSupport">
		<wsag:All >
			<wsag:ServiceDescriptionTerm wsag:Name="SDT_AWS-S3PremiumSupport" wsag:ServiceName="AWS-S3PremiumSupport" >
				<OfferItem name="ObjectSize" wsag:Metric="metrics/S3Premium_1403021096598:float" >7</OfferItem>
				<OfferItem name="Region" wsag:Metric="metrics/S3Premium_1403021096598:countries" ></OfferItem>
				<OfferItem name="Interface" wsag:Metric="metrics/S3Premium_1403021096598:interfaceType" ></OfferItem>
				<OfferItem name="SVN" wsag:Metric="metrics/S3Premium_1403021096598:boolean" ></OfferItem>
				<OfferItem name="RRS" wsag:Metric="metrics/S3Premium_1403021096598:boolean" ></OfferItem>
				<OfferItem name="TotalPrice" wsag:Metric="metrics/S3Premium_1403021096598:money" >100</OfferItem>
				<OfferItem name="StoragePrice" wsag:Metric="metrics/S3Premium_1403021096598:money" ></OfferItem>
				<OfferItem name="DataTransferPrice" wsag:Metric="metrics/S3Premium_1403021096598:money" ></OfferItem>
				<OfferItem name="StorageSize" wsag:Metric="metrics/S3Premium_1403021096598:size" ></OfferItem>
				<OfferItem name="WriteRequests" wsag:Metric="metrics/S3Premium_1403021096598:integer" ></OfferItem>
				<OfferItem name="ReadRequests" wsag:Metric="metrics/S3Premium_1403021096598:integer" ></OfferItem>
				<OfferItem name="TransferredGB" wsag:Metric="metrics/S3Premium_1403021096598:size" ></OfferItem>
				<OfferItem name="Support" wsag:Metric="metrics/S3Premium_1403021096598:supportType" ></OfferItem>
			</wsag:ServiceDescriptionTerm>

			<wsag:ServiceReference  wsag:Name="AWS-S3PremiumSupport_SREF" wsag:ServiceName="AWS-S3PremiumSupport" >http://aws.amazon.com/s3/</wsag:ServiceReference>

			<wsag:ServiceProperties wsag:Name="SP_AWS-S3PremiumSupport" wsag:ServiceName="AWS-S3PremiumSupport">
				<wsag:VariableSet >
					<wsag:Variable  wsag:Name="Availability" wsag:Metric="metrics/S3Premium_1403021096598:percent" >
						<wsag:Location >/Availability</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="DataDurability" wsag:Metric="metrics/S3Premium_1403021096598:percent" >
						<wsag:Location >/DataDurability</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="ForumAccess" wsag:Metric="metrics/S3Premium_1403021096598:boolean" >
						<wsag:Location >/ForumAccess</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="Report-an-issue" wsag:Metric="metrics/S3Premium_1403021096598:boolean" >
						<wsag:Location >/Report-an-issue</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="WebTicketing" wsag:Metric="metrics/S3Premium_1403021096598:boolean" >
						<wsag:Location >/WebTicketing</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="WebSupport" wsag:Metric="metrics/S3Premium_1403021096598:boolean" >
						<wsag:Location >/WebSupport</wsag:Location>
					</wsag:Variable >
				</wsag:VariableSet >
			</wsag:ServiceProperties>

			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G1" >
				<wsag:QualifyingCondition >
					RegionUnits &gt; 2
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective>
					<wsag:CustomServiceLevel>MUP &gt;= 99.95</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
				<wsag:BusinessValueList>
					<wsag:Penalty>
						<wsag:AssessmentInterval>
							<wsag:TimeInterval>monthly</wsag:TimeInterval>
						</wsag:AssessmentInterval>
						<wsag:ValueExpr>SCP = 10 if MUP &lt;= 99 AND MUP &lt; 99.95</wsag:ValueExpr>
					</wsag:Penalty>
					<wsag:Penalty>
						<wsag:AssessmentInterval>
							<wsag:TimeInterval>monthly</wsag:TimeInterval>
						</wsag:AssessmentInterval>
						<wsag:ValueExpr>SCP = 20 if MUP &lt; 99</wsag:ValueExpr>
					</wsag:Penalty>
				</wsag:BusinessValueList>
				<wsag:BusinessValueList>
					<wsag:Reward>
						<wsag:AssessmentInterval>
							<wsag:TimeInterval>yearly</wsag:TimeInterval>
						</wsag:AssessmentInterval>
						<wsag:ValueExpr>SCP = 15 if MUP &gt;= 85</wsag:ValueExpr>
					</wsag:Reward>
				</wsag:BusinessValueList>
			</wsag:GuaranteeTerm>

		</wsag:All >
	</wsag:Terms >

	<wsag:CreationConstraints >
		<wsag:Constraint >
			<Name>C1</Name>
			<Content>(Region = "US" OR Region = "west_US" OR Region = "EU_Irland" OR Region = "Pacific_Asia" OR Region = "Southamerica" OR Region = "GovCloud_US")</Content>
		</wsag:Constraint >
	</wsag:CreationConstraints >
</wsag:Template>