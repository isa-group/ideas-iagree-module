<met:metricXML xmlns:met="http:/www.isa.us.es/ada/metrics">
	<met:metric id="boolean" type="enumerated" >
		<met:value value="True"/>
		<met:value value="False"/>
	</met:metric>
	<met:metric id="id" type="integer"  min="0" max="500"/>
	<met:metric id="percent" type="integer"  min="0" max="100" />
	<met:metric id="countries" type="enumerated" >
		<met:value value="US"/>
		<met:value value="west_US"/>
		<met:value value="EU_Irland"/>
		<met:value value="Pacific_Asia"/>
		<met:value value="Southamerica"/>
		<met:value value="GovCloud_US"/>
	</met:metric>
	<met:metric id="interfaceType" type="enumerated" >
		<met:value value="SOAP"/>
		<met:value value="REST"/>
	</met:metric>
	<met:metric id="objectType" type="integer"  min="0" max="500"/>
	<met:metric id="protocol" type="enumerated" >
		<met:value value="HTTP"/>
		<met:value value="BitTorrent"/>
		<met:value value="HTTPS"/>
		<met:value value="FTP"/>
	</met:metric>
	<met:metric id="rightsType" type="enumerated" >
		<met:value value="read"/>
		<met:value value="write"/>
		<met:value value="delete"/>
		<met:value value="read_write"/>
		<met:value value="full"/>
	</met:metric>
	<met:metric id="money" type="integer"  min="0" max="500"/>
	<met:metric id="size" type="integer"  min="0" max="500"/>
	<met:metric id="supportType" type="enumerated" >
		<met:value value="Bronze"/>
		<met:value value="Silver"/>
		<met:value value="Gold"/>
		<met:value value="Platinum"/>
	</met:metric>
	<met:metric id="Boolean" type="enumerated"  min="0" max="500"/>
	<met:metric id="RangeTest" type="integer"  min="-20" max="100" />
</met:metricXML>

