<?xml version="1.0" encoding = "UTF-8"?>
<wsag:AgreementOffer wsag:AgreementId="1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
 xmlns:wsag="http://schemas.ggf.org/graap/2007/03/ws-agreement" 
 xmlns:xs="http://www.w3.org/2001/XMLSchema" 
 xsi:schemaLocation="http://schemas.ggf.org/graap/2007/03/wsagreement" 
 agreement_types.xsd="http://www.w3.org/2001/XMLSchema XMLSchema.xsd" >
	<wsag:Name >TranslationServiceOffer</wsag:Name>
	<wsag:Context >
		<wsag:TemplateId>1.0</wsag:TemplateId>
		<wsag:TemplateName>TranslationService</wsag:TemplateName>
		<wsag:AgreementResponder >Provider</wsag:AgreementResponder >

	</wsag:Context >

	<wsag:Terms wsag:Name="TranslationService">
		<wsag:All >
			<wsag:ServiceDescriptionTerm wsag:Name="SDT_TranslationService" wsag:ServiceName="TranslationService" >
				<OfferItem name="TranslationLangs" wsag:Metric="metrics/TranslationServiceOffer_1398072330915:translations" ></OfferItem>
				<OfferItem name="InputFileSize" wsag:Metric="metrics/TranslationServiceOffer_1398072330915:int" ></OfferItem>
				<OfferItem name="ImageTranslation" wsag:Metric="metrics/TranslationServiceOffer_1398072330915:boolean" ></OfferItem>
				<OfferItem name="Cost" wsag:Metric="metrics/TranslationServiceOffer_1398072330915:int" ></OfferItem>
			</wsag:ServiceDescriptionTerm>

			<wsag:ServiceProperties wsag:Name="SP_TranslationService" wsag:ServiceName="TranslationService">
				<wsag:VariableSet >
					<wsag:Variable  wsag:Name="TranslationTime" wsag:Metric="metrics/TranslationServiceOffer_1398072330915:time" >
						<wsag:Location >/TranslationTime</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="InputErrors" wsag:Metric="metrics/TranslationServiceOffer_1398072330915:percent" >
						<wsag:Location >/InputErrors</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="OutputErrors" wsag:Metric="metrics/TranslationServiceOffer_1398072330915:percent" >
						<wsag:Location >/OutputErrors</wsag:Location>
					</wsag:Variable >
				</wsag:VariableSet >
			</wsag:ServiceProperties>

			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G1" >
				<wsag:QualifyingCondition >
					ImageTranslation="False"
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >TranslationTime&lt;=1</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G2" >
				<wsag:QualifyingCondition >
					ImageTranslation="True"
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >TranslationTime&lt;=2</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G4" >
				<wsag:QualifyingCondition >
					InputErrors&gt;1
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >TranslationTime&lt;=1</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceConsumer" wsag:Name="G6" >
				<wsag:QualifyingCondition >
					TranslationLangs="ES_to_EN-UK"
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >InputFileSize&gt;=30</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceConsumer" wsag:Name="G7" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >InputFileSize&lt;30</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:OneOrMore >
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G9.1" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >OutputErrors&lt;=1</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceConsumer" wsag:Name="G9.2" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >InputErrors&lt;=1</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceConsumer" wsag:Name="G9.3" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >InputErrors&gt;1</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			</wsag:OneOrMore >

		</wsag:All >
	</wsag:Terms >

</wsag:AgreementOffer>