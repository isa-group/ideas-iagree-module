<?xml version="1.0" encoding = "UTF-8"?>
<wsag:Template wsag:TemplateId="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
 xmlns:wsag="http://schemas.ggf.org/graap/2007/03/ws-agreement" 
 xmlns:xs="http://www.w3.org/2001/XMLSchema" 
 xsi:schemaLocation="http://schemas.ggf.org/graap/2007/03/wsagreement" 
 agreement_types.xsd="http://www.w3.org/2001/XMLSchema XMLSchema.xsd" >
	<wsag:Name >TranslationServiceForCompliance</wsag:Name>
	<wsag:Context >
		<wsag:AgreementResponder >Provider</wsag:AgreementResponder >

	</wsag:Context >

	<wsag:Terms wsag:Name="TranslationServiceForCompliance">
		<wsag:All >
			<wsag:ServiceDescriptionTerm wsag:Name="SDT_TranslationServiceForCompliance" wsag:ServiceName="TranslationServiceForCompliance" >
				<OfferItem name="TranslationLangs" wsag:Metric="metrics/TranslationServiceForCompliance_1398071165688:translations" >"ES_to_EN-UK"</OfferItem>
				<OfferItem name="InputFileSize" wsag:Metric="metrics/TranslationServiceForCompliance_1398071165688:int" ></OfferItem>
				<OfferItem name="ImageTranslation" wsag:Metric="metrics/TranslationServiceForCompliance_1398071165688:boolean" ></OfferItem>
				<OfferItem name="Cost" wsag:Metric="metrics/TranslationServiceForCompliance_1398071165688:int" ></OfferItem>
			</wsag:ServiceDescriptionTerm>

			<wsag:ServiceProperties wsag:Name="SP_TranslationServiceForCompliance" wsag:ServiceName="TranslationServiceForCompliance">
				<wsag:VariableSet >
					<wsag:Variable  wsag:Name="TranslationTime" wsag:Metric="metrics/TranslationServiceForCompliance_1398071165688:time" >
						<wsag:Location >/TranslationTime</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="InputErrors" wsag:Metric="metrics/TranslationServiceForCompliance_1398071165688:percent" >
						<wsag:Location >/InputErrors</wsag:Location>
					</wsag:Variable >
				</wsag:VariableSet >
			</wsag:ServiceProperties>

			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G1" >
				<wsag:QualifyingCondition >
					ImageTranslation="False"
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >TranslationTime&lt;=1</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G2" >
				<wsag:QualifyingCondition >
					ImageTranslation="True"
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >TranslationTime&lt;=2</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceConsumer" wsag:Name="G3" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >InputErrors&lt;=1</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>

		</wsag:All >
	</wsag:Terms >

	<wsag:CreationConstraints >
		<wsag:Constraint >
			<Name>C1</Name>
			<Content>(TranslationLangs = "ES_to_EN-UK") OR (TranslationLangs = "ES_to_FR") OR (TranslationLangs = "EN-UK_to_ES") OR (TranslationLangs = "FR_to_ES") OR (TranslationLangs = "Auto"))</Content>
		</wsag:Constraint >
		<wsag:Constraint >
			<Name>C2</Name>
			<Content>InputFileSize&lt;=50</Content>
		</wsag:Constraint >
		<wsag:Constraint >
			<Name>C3</Name>
			<Content>Cost&gt;=0</Content>
		</wsag:Constraint >
		<wsag:Constraint >
			<Name>C4</Name>
			<Content>ImageTranslation="False" IMPLIES Cost=InputFileSize*1</Content>
		</wsag:Constraint >
		<wsag:Constraint >
			<Name>C5</Name>
			<Content>ImageTranslation="True" IMPLIES Cost=InputFileSize*2</Content>
		</wsag:Constraint >
		<wsag:Constraint >
			<Name>C6</Name>
			<Content>InputFileSize&lt;10 IMPLIES ImageTranslation="False"</Content>
		</wsag:Constraint >
	</wsag:CreationConstraints >
</wsag:Template>