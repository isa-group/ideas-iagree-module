<?xml version="1.0" encoding = "UTF-8"?>
<wsag:Template wsag:TemplateId="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
 xmlns:wsag="http://schemas.ggf.org/graap/2007/03/ws-agreement" 
 xmlns:xs="http://www.w3.org/2001/XMLSchema" 
 xsi:schemaLocation="http://schemas.ggf.org/graap/2007/03/wsagreement" 
 agreement_types.xsd="http://www.w3.org/2001/XMLSchema XMLSchema.xsd" >
	<wsag:Name >SALMonMonitoringService</wsag:Name>
	<wsag:Context >
		<wsag:AgreementResponder >Provider</wsag:AgreementResponder >

	</wsag:Context >

	<wsag:Terms wsag:Name="MonitoringService">
		<wsag:All >
			<wsag:ServiceDescriptionTerm wsag:Name="SDT_MonitoringService" wsag:ServiceName="MonitoringService" >
				<OfferItem name="Organisation" wsag:Metric="metrics/SALMonMonitoringService_1398072568530:organisations" ></OfferItem>
				<OfferItem name="RequestsPerSec" wsag:Metric="metrics/SALMonMonitoringService_1398072568530:requests" ></OfferItem>
				<OfferItem name="ResponseTime" wsag:Metric="metrics/SALMonMonitoringService_1398072568530:seconds" ></OfferItem>
			</wsag:ServiceDescriptionTerm>

			<wsag:ServiceProperties wsag:Name="SP_MonitoringService" wsag:ServiceName="MonitoringService">
				<wsag:VariableSet >
					<wsag:Variable  wsag:Name="Organisation" wsag:Metric="metrics/SALMonMonitoringService_1398072568530:organisations" >
						<wsag:Location >/Organisation</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="RequestsPerSec" wsag:Metric="metrics/SALMonMonitoringService_1398072568530:requests" >
						<wsag:Location >/RequestsPerSec</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="ResponseTime" wsag:Metric="metrics/SALMonMonitoringService_1398072568530:seconds" >
						<wsag:Location >/ResponseTime</wsag:Location>
					</wsag:Variable >
				</wsag:VariableSet >
			</wsag:ServiceProperties>

			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G1" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >(ResponseTime&gt;=1) AND (ResponseTime&lt;=10)</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G2" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >(RequestsPerSec&gt;=4)</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G3" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >(RequestsPerSec&lt;=20)</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:ExactlyOne >
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G4.1" >
				<wsag:QualifyingCondition >
					Organisation="ISA" AND Organisation="GESSI"
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >RequestsPerSec&gt;=10</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G4.2" >
				<wsag:QualifyingCondition >
					Organisation="GESSI"
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >ResponseTime&lt;=5</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			</wsag:ExactlyOne >

		</wsag:All >
	</wsag:Terms >

	<wsag:CreationConstraints >
	</wsag:CreationConstraints >
</wsag:Template>