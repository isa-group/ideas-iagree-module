<?xml version="1.0" encoding = "UTF-8"?>
<wsag:AgreementOffer wsag:AgreementId="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
 xmlns:wsag="http://schemas.ggf.org/graap/2007/03/ws-agreement" 
 xmlns:xs="http://www.w3.org/2001/XMLSchema" 
 xsi:schemaLocation="http://schemas.ggf.org/graap/2007/03/wsagreement" 
 agreement_types.xsd="http://www.w3.org/2001/XMLSchema XMLSchema.xsd" >
	<wsag:Name >CompliantOffer</wsag:Name>
	<wsag:Context >
		<wsag:TemplateId>1.0</wsag:TemplateId>
		<wsag:TemplateName>TranslationServiceForCompliance</wsag:TemplateName>
		<wsag:AgreementResponder >Provider</wsag:AgreementResponder >

	</wsag:Context >

	<wsag:Terms wsag:Name="TranslationServiceForCompliance">
		<wsag:All >
			<wsag:ServiceDescriptionTerm wsag:Name="SDT_TranslationServiceForCompliance" wsag:ServiceName="TranslationServiceForCompliance" >
				<OfferItem name="TranslationLangs" wsag:Metric="metrics/CompliantOffer_1398070687321:translations" >"ES_to_EN-UK"</OfferItem>
				<OfferItem name="InputFileSize" wsag:Metric="metrics/CompliantOffer_1398070687321:int" >9</OfferItem>
				<OfferItem name="ImageTranslation" wsag:Metric="metrics/CompliantOffer_1398070687321:boolean" >"False"</OfferItem>
				<OfferItem name="Cost" wsag:Metric="metrics/CompliantOffer_1398070687321:int" >9</OfferItem>
			</wsag:ServiceDescriptionTerm>

			<wsag:ServiceProperties wsag:Name="SP_TranslationServiceForCompliance" wsag:ServiceName="TranslationServiceForCompliance">
				<wsag:VariableSet >
					<wsag:Variable  wsag:Name="TranslationTime" wsag:Metric="metrics/CompliantOffer_1398070687321:time" >
						<wsag:Location >/TranslationTime</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="InputErrors" wsag:Metric="metrics/CompliantOffer_1398070687321:percent" >
						<wsag:Location >/InputErrors</wsag:Location>
					</wsag:Variable >
				</wsag:VariableSet >
			</wsag:ServiceProperties>

			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G1" >
				<wsag:QualifyingCondition >
					ImageTranslation="False"
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >TranslationTime&lt;=1</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G2" >
				<wsag:QualifyingCondition >
					ImageTranslation="True"
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >TranslationTime&lt;=2</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceConsumer" wsag:Name="G3" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >InputErrors&lt;=1</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>

		</wsag:All >
	</wsag:Terms >

</wsag:AgreementOffer>