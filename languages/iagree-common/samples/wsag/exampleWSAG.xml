<?xml version="1.0" encoding = "UTF-8"?>
<wsag:Template wsag:TemplateId="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
 xmlns:wsag="http://schemas.ggf.org/graap/2007/03/ws-agreement" 
 xmlns:xs="http://www.w3.org/2001/XMLSchema" 
 xsi:schemaLocation="http://schemas.ggf.org/graap/2007/03/wsagreement" 
 agreement_types.xsd="http://www.w3.org/2001/XMLSchema XMLSchema.xsd" >
	<wsag:Name >S3Premium</wsag:Name>
	<wsag:Context >
		<wsag:AgreementResponder >Provider</wsag:AgreementResponder >

	</wsag:Context >

	<wsag:Terms wsag:Name="AWS-S3PremiumSupport">
		<wsag:All >
			<wsag:ServiceDescriptionTerm wsag:Name="SDT_AWS-S3PremiumSupport" wsag:ServiceName="AWS-S3PremiumSupport" >
				<OfferItem name="ObjectSize" wsag:Metric="metrics/S3Premium_1336591850145:integer" ></OfferItem>
				<OfferItem name="Region" wsag:Metric="metrics/S3Premium_1336591850145:countries" ></OfferItem>
				<OfferItem name="Interface" wsag:Metric="metrics/S3Premium_1336591850145:interfaceType" ></OfferItem>
				<OfferItem name="SVN" wsag:Metric="metrics/S3Premium_1336591850145:boolean" ></OfferItem>
				<OfferItem name="RRS" wsag:Metric="metrics/S3Premium_1336591850145:boolean" ></OfferItem>
				<OfferItem name="TotalPrice" wsag:Metric="metrics/S3Premium_1336591850145:money" ></OfferItem>
				<OfferItem name="StoragePrice" wsag:Metric="metrics/S3Premium_1336591850145:money" ></OfferItem>
				<OfferItem name="DataTransferPrice" wsag:Metric="metrics/S3Premium_1336591850145:money" ></OfferItem>
				<OfferItem name="StorageSize" wsag:Metric="metrics/S3Premium_1336591850145:size" ></OfferItem>
				<OfferItem name="WriteRequests" wsag:Metric="metrics/S3Premium_1336591850145:integer" ></OfferItem>
				<OfferItem name="ReadRequests" wsag:Metric="metrics/S3Premium_1336591850145:integer" ></OfferItem>
				<OfferItem name="TransferredGB" wsag:Metric="metrics/S3Premium_1336591850145:size" ></OfferItem>
				<OfferItem name="Support" wsag:Metric="metrics/S3Premium_1336591850145:supportType" ></OfferItem>
			</wsag:ServiceDescriptionTerm>

			<wsag:ServiceReference  wsag:Name="AWS-S3PremiumSupport_SREF" wsag:ServiceName="AWS-S3PremiumSupport" >http://aws.amazon.com/s3/</wsag:ServiceReference>

			<wsag:ServiceProperties wsag:Name="SP_AWS-S3PremiumSupport" wsag:ServiceName="AWS-S3PremiumSupport">
				<wsag:VariableSet >
					<wsag:Variable  wsag:Name="Availability" wsag:Metric="metrics/S3Premium_1336591850145:percent" >
						<wsag:Location >/Availability</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="DataDurability" wsag:Metric="metrics/S3Premium_1336591850145:percent" >
						<wsag:Location >/DataDurability</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="ForumAccess" wsag:Metric="metrics/S3Premium_1336591850145:boolean" >
						<wsag:Location >/ForumAccess</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="ReportAnIssue" wsag:Metric="metrics/S3Premium_1336591850145:boolean" >
						<wsag:Location >/ReportAnIssue</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="WebTicketing" wsag:Metric="metrics/S3Premium_1336591850145:boolean" >
						<wsag:Location >/WebTicketing</wsag:Location>
					</wsag:Variable >
					<wsag:Variable  wsag:Name="WebSupport" wsag:Metric="metrics/S3Premium_1336591850145:boolean" >
						<wsag:Location >/WebSupport</wsag:Location>
					</wsag:Variable >
				</wsag:VariableSet >
			</wsag:ServiceProperties>

			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G2" >
				<wsag:QualifyingCondition >
					RRS = "False"
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >DataDurability &gt;= 99</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G3" >
				<wsag:QualifyingCondition >
					RRS = "True"
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >DataDurability &lt;= 98</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G4" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >ForumAccess = "True" AND ReportAnIssue = "True"</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G5" >
				<wsag:QualifyingCondition >
					Support = "Bronze"
				</wsag:QualifyingCondition>
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >WebTicketing = "True" AND WebSupport = "True"</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:OneOrMore >
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G6.1" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >(WebTicketing = "True" AND WebSupport = "True") IMPLIES Support = "Bronze"</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G6.2" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >(TurnAroundTime = 240) IMPLIES (Support = "Silver" AND WebTicketing = "True" AND WebSupport = "True")</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			</wsag:OneOrMore >

		</wsag:All >
	</wsag:Terms >

	<wsag:CreationConstraints >
		<wsag:Constraint >
			<Name>C1</Name>
			<Content>(Region = US OR Region = "west_US" OR Region = "EU_Irland" OR Region = "Pacific_Asia" OR Region = "Southamerica" OR Region = "GovCloud_US")</Content>
		</wsag:Constraint >
		<wsag:Constraint >
			<Name>C3</Name>
			<Content>StorageSize &lt;= 5 AND WriteRequests &lt;= 400 AND ReadRequests &lt;= 400 AND TransferredGB &lt;= 15 IMPLIES TotalPrice = 0</Content>
		</wsag:Constraint >
	</wsag:CreationConstraints >
</wsag:Template>