<?xml version="1.0" encoding = "UTF-8"?>
<wsag:AgreementOffer wsag:AgreementId="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
 xmlns:wsag="http://schemas.ggf.org/graap/2007/03/ws-agreement" 
 xmlns:xs="http://www.w3.org/2001/XMLSchema" 
 xsi:schemaLocation="http://schemas.ggf.org/graap/2007/03/wsagreement" 
 agreement_types.xsd="http://www.w3.org/2001/XMLSchema XMLSchema.xsd" >
	<wsag:Name >CompliantOfferAmazon</wsag:Name>
	<wsag:Context >
		<wsag:TemplateId>1.0</wsag:TemplateId>
		<wsag:TemplateName>AmazonS3TemplateForCompliance</wsag:TemplateName>
		<wsag:AgreementResponder >Provider</wsag:AgreementResponder >

	</wsag:Context >

	<wsag:Terms wsag:Name="AWS-S3">
		<wsag:All >
			<wsag:ServiceDescriptionTerm wsag:Name="SDT_AWS-S3" wsag:ServiceName="AWS-S3" >
				<OfferItem name="Interface" wsag:Metric="metrics/compliantOfferMetrics:interfaces" >"SOAP"</OfferItem>
			</wsag:ServiceDescriptionTerm>

			<wsag:ServiceProperties wsag:Name="SP_AWS-S3" wsag:ServiceName="AWS-S3">
				<wsag:VariableSet >
					<wsag:Variable  wsag:Name="MUP" wsag:Metric="metrics/compliantOfferMetrics:percent" >
						<wsag:Location >/MUP</wsag:Location>
					</wsag:Variable >
				</wsag:VariableSet >
			</wsag:ServiceProperties>

			<wsag:GuaranteeTerm wsag:Obligated="ServiceProvider" wsag:Name="G1" >
				<wsag:ServiceLevelObjective >
					<wsag:CustomServiceLevel >MUP&gt;=98</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>

		</wsag:All >
	</wsag:Terms >

</wsag:AgreementOffer>